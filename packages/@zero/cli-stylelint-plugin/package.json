{
  "name": "@zero-cli/cli-stylelint-plugin",
  "version": "2.0.3",
  "description": "cli-stylelint-plugin",
  "keywords": [
    "stylelint",
    "plugin"
  ],
  "author": "xiaocai <894516496@qq.com>",
  "homepage": "https://github.com/13426078416/zero-cli/blob/master/packages/%40zero/cli-stylelint-plugin/README.md",
  "license": "MIT",
  "main": "dist/index.js",
